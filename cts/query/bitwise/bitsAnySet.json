{
  "integer-bitmask": {
    "request": {
      "find": "values",
      "filter": {
        "v": {
          "$bitsAnySet": {
            "$numberInt": "6"
          }
        }
      },
      "sort": {
        "_id": {
          "$numberInt": "1"
        }
      },
      "$db": "{{.Database}}"
    },
    "response": {
      "cursor": {
        "firstBatch": [
          {
            "_id": "binary",
            "_comment": "The base64 encoded string of bytes [42, 0, 13].",
            "v": {
              "$binary": {
                "base64": "KgAN",
                "subType": "80"
              }
            }
          },
          {
            "_id": "decimal128-whole",
            "v": {
              "$numberDecimal": "42"
            }
          },
          {
            "_id": "double-max-integer",
            "_comment": "The largest integer value representable as a double.",
            "v": {
              "$numberDouble": "9.007199254740991E+15"
            }
          },
          {
            "_id": "double-whole",
            "v": {
              "$numberDouble": "42.0"
            }
          },
          {
            "_id": "int32",
            "v": {
              "$numberInt": "42"
            }
          },
          {
            "_id": "int32-max",
            "v": {
              "$numberInt": "2147483647"
            }
          },
          {
            "_id": "int64",
            "v": {
              "$numberLong": "42"
            }
          },
          {
            "_id": "int64-max",
            "v": {
              "$numberLong": "9223372036854775807"
            }
          }
        ],
        "id": {
          "$numberLong": "0"
        },
        "ns": "{{.Database}}.values"
      },
      "ok": {
        "$numberDouble": "1.0"
      }
    }
  }
}
