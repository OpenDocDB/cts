{
  "double": {
    "request": {
      "find": "values",
      "filter": {
        "$and": [
          {
            "v": {
              "$gte": {
                "$numberDouble": "0.0"
              }
            }
          },
          {
            "v": {
              "$lte": {
                "$numberDouble": "42.0"
              }
            }
          }
        ]
      },
      "sort": {
        "_id": {
          "$numberInt": "1"
        }
      },
      "$db": "{{.Database}}"
    },
    "response": {
      "cursor": {
        "firstBatch": [
          {
            "_id": "decimal128-negative-zero",
            "v": {
              "$numberDecimal": "-0.0"
            }
          },
          {
            "_id": "decimal128-whole",
            "v": {
              "$numberDecimal": "42"
            }
          },
          {
            "_id": "decimal128-zero",
            "v": {
              "$numberDecimal": "0.0"
            }
          },
          {
            "_id": "double-negative-zero",
            "v": {
              "$numberDouble": "-0.0"
            }
          },
          {
            "_id": "double-smallest-non-zero",
            "_comment": "The smallest positive non-zero value representable as a double.",
            "v": {
              "$numberDouble": "5E-324"
            }
          },
          {
            "_id": "double-whole",
            "v": {
              "$numberDouble": "42.0"
            }
          },
          {
            "_id": "double-zero",
            "v": {
              "$numberDouble": "0.0"
            }
          },
          {
            "_id": "int32",
            "v": {
              "$numberInt": "42"
            }
          },
          {
            "_id": "int32-zero",
            "v": {
              "$numberInt": "0"
            }
          },
          {
            "_id": "int64",
            "v": {
              "$numberLong": "42"
            }
          },
          {
            "_id": "int64-zero",
            "v": {
              "$numberLong": "0"
            }
          }
        ],
        "id": {
          "$numberLong": "0"
        },
        "ns": "{{.Database}}.values"
      },
      "ok": {
        "$numberDouble": "1.0"
      }
    }
  }
}
