{
  "2-find": {
    "request": {
      "find": "values",
      "filter": {
        "_id": "int32"
      },
      "sort": {
        "_id": {
          "$numberInt": "1"
        }
      },
      "$db": "{{.Database}}"
    },
    "response": {
      "cursor": {
        "firstBatch": [
          {
            "_id": "int32",
            "v": {
              "$numberInt": "42"
            }
          }
        ],
        "id": {
          "$numberLong": "0"
        },
        "ns": "{{.Database}}.values"
      },
      "ok": {
        "$numberDouble": "1.0"
      }
    }
  },
  "or-number": {
    "request": {
      "update": "values",
      "updates": [
        {
          "q": {
            "_id": "int32"
          },
          "u": {
            "$bit": {
              "v": {
                "or": {
                  "$numberInt": "4"
                }
              }
            }
          }
        }
      ],
      "$db": "{{.Database}}"
    },
    "response": {
      "n": {
        "$numberInt": "1"
      },
      "nModified": {
        "$numberInt": "1"
      },
      "ok": {
        "$numberDouble": "1.0"
      }
    }
  }
}
