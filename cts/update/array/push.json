{
  "1-number": {
    "request": {
      "update": "composites",
      "updates": [
        {
          "q": {
            "_id": "array"
          },
          "u": {
            "$push": {
              "v": {
                "$numberInt": "100"
              }
            }
          }
        }
      ],
      "$db": "{{.Database}}"
    },
    "response": {
      "n": {
        "$numberInt": "0"
      },
      "nModified": {
        "$numberInt": "0"
      },
      "ok": {
        "$numberDouble": "1.0"
      }
    }
  },
  "2-find": {
    "request": {
      "find": "composites",
      "filter": {
        "_id": "array"
      },
      "sort": {
        "_id": {
          "$numberInt": "1"
        }
      },
      "$db": "{{.Database}}"
    },
    "response": {
      "cursor": {
        "firstBatch": [],
        "id": {
          "$numberLong": "0"
        },
        "ns": "{{.Database}}.values"
      },
      "ok": {
        "$numberDouble": "1.0"
      }
    }
  }
}