{
    "name": "cts",
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],
    "service": "devcontainer",
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.go"
            ]
        },
        "files": {
            "insertFinalNewline": true,
            "trimTrailingWhitespace": true
        }
    },
    "workspaceFolder": "/workspace",
    "overrideCommand": true,
    "remoteUser": "root",
    "onCreateCommand": "cd /workspace/opendocdb-cts && make init"
}
